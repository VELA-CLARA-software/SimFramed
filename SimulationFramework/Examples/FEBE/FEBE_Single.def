global:
  SC_2D_Nrad: 30
  SC_2D_Nlong: 45
  SC_3D_Nxf: 8
  SC_3D_Nyf: 8
  SC_3D_Nzf: 8
  accuracy: 6
  initial_distribution: '../1k-250pC-76fsrms-1mm_TE09fixN12.ini'
  ASTRAsettings:
    High_res: True
    N_red: 1
    EmitS: True
    PhaseS: True
    TrackS: True
    RefS: True
    Track_All: True
    Phase_Scan: False
    Auto_Phase: True
    check_ref_part: False
    Zphase: 1
    Zemit: 1050
    H_max: 0.0007
    H_min: 0.0007
    title: 'trial'
    run_no: 1
    Smooth_x: 2
    Smooth_y: 2
    Smooth_z: 2
    Max_scale: 0.1
    Max_count: 100
    cell_var: 2
    min_grid: 3.424657e-13
    Lmagnetized: False
generator:
  default: clara_400_3ps
files:
  injector400:
    code: ASTRA
    charge:
      cathode: True
      space_charge_mode: 2D
      mirror_charge: True
    input:
      particle_definition: 'initial_distribution'
    output:
      zstart: 0
      end_element: CLA-S02-APER-01
  PreFEBE:
    code: ASTRA
    charge:
      cathode: False
      space_charge_mode: 3D
    output:
      start_element: CLA-S02-APER-01
      end_element: CLA-FEB-START-01
    groups:
      laser-heater:
        dipoleangle: 0.0001
        width: 0.2
        gap: 0.02
  FEBE:
    code: elegant
    charge:
      cathode: False
      space_charge_mode: 3D
    input: {}
    output:
      start_element: CLA-FEB-START-01
      end_element: CLA-FEB-DUMP-01-START
groups:
  laser-heater:
    type: chicane
    elements: [CLA-S04-MAG-DIP-01, CLA-S04-MAG-DIP-02, CLA-S04-MAG-DIP-03, CLA-S04-MAG-DIP-04]
  bunch_compressor:
    type: chicane
    elements: [CLA-VBC-MAG-DIP-01, CLA-VBC-MAG-DIP-02, CLA-VBC-MAG-DIP-03, CLA-VBC-MAG-DIP-04]
  FODO_F:
    type: element_group
    elements: [CLA-FEB-MAG-QUAD-01, CLA-FEB-MAG-QUAD-03, CLA-FEB-MAG-QUAD-05, CLA-FEB-MAG-QUAD-07, CLA-FEB-MAG-QUAD-09, CLA-FEB-MAG-QUAD-11]
  FODO_D:
    type: element_group
    elements: [CLA-FEB-MAG-QUAD-02, CLA-FEB-MAG-QUAD-04, CLA-FEB-MAG-QUAD-06, CLA-FEB-MAG-QUAD-08, CLA-FEB-MAG-QUAD-10, CLA-FEB-MAG-QUAD-12]
elements:
  filename: [YAML/Injector400.yaml, YAML/S02.yaml, YAML/L02.yaml,
             YAML/S03.yaml, YAML/L03.yaml, YAML/S04.yaml, YAML/L4H.yaml,
             YAML/S05.yaml, YAML/VBC.yaml, YAML/S06.yaml, YAML/L04.yaml,
             YAML/S07.yaml, YAML/FEBE.yaml]
